                    android:maxLines="2"
                    android:padding="5dp"
                    android:text="00000"
                    android:textAppearance="?android:attr/textAppearanceSmall" />
            </TableRow>
        </TableLayout>

        <TextView
            android:id="@+id/textViewTotalBalance"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textViewName"
            android:layout_below="@+id/textViewPlace"
            android:layout_margin="3dp"
            android:maxLines="2"
            android:text="Total payment balance : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textViewTotalBalanceData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textViewPlace"
            android:layout_margin="3dp"
            android:layout_toRightOf="@+id/textViewTotalBalance"
            android:gravity="right"
            android:maxLines="2"
            android:text="00000"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textViewTotalDue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textViewName"
            android:layout_below="@+id/textViewTotalBalance"
            android:layout_margin="3dp"
            android:maxLines="2"
            android:text="Total payment due : "
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textViewTotalDueData"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textViewTotalBalance"
            android:layout_margin="3dp"
            android:layout_toRightOf="@+id/textViewTotalDue"
            android:gravity="right"
            android:maxLines="2"
            android:text="00000"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </RelativeLayout>

</RelativeLayout>